<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bean.SightType" table="sight_type">
		<id name="sightTypeId" type="java.lang.Integer">
			<column name="sight_type_id" />
			<generator class="increment" />
		</id>
		
		<property name="sightId" type="java.lang.Integer">
			<column name="sight_id" />
		</property>
		
		<property name="sightType" type="java.lang.String">
			<column name="sight_type" />
		</property>
		
	</class>
</hibernate-mapping>