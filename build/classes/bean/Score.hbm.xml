<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bean.Score" table="score">
		<id name="scoreId" type="java.lang.Integer">
			<column name="score_id" />
			<generator class="increment" />
		</id>
		<property name="sightId" type="java.lang.Integer">
			<column name="sight_id"  />
		</property>
		<property name="userId" type="java.lang.Integer">
			<column name="user_id"   />
		</property>
		
		<property name="point" type="java.lang.Integer">
			<column name="point" />
		</property>
		
		<property name="scoreMsg" type="java.lang.String">
			<column name="score_msg" />
		</property>
		
	</class>
</hibernate-mapping>
