		<action name="reminderOutput" class="reminderOutputAction">
			<interceptor-ref name="defaultStack"></interceptor-ref>
			<result name="success" type="json">
				<param name="includeProperties">
					error_type,error_message
				</param>
			</result>
		</action>
		

	</package>
</struts>    
